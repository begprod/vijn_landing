.header
	.container.container_wide.header__inner
		.header__logo
			a(href="/")
				img(src="./assets/images/logo.svg" alt="vijn")
		.header__menu.js-menu-container
			button(type="button").button.button_view_transparent.button_size_sm.header__menu-trigger.js-menu-trigger
				span Menu
				.header__menu-trigger-icon
					span
			nav.menu
				ul.menu__inner
					li.menu__item
						a(href="#").menu__link Release notes
					li.menu__item
						a(href="#").menu__link Blog
					li.menu__item
						a(href="#").menu__link About us
					li.menu__item
						a(href="./faq.html").menu__link FAQ
					li.menu__item
						a(href="#").menu__link User guide
					li.menu__item
						a(href="#").button.button_view_transparent.button_size_sm.menu__link.menu__link_button Register
					li.menu__item
						a(href="#").menu__link  Log in
			.header__overlay.js-menu-overlay